<Window x:Class="Autolect.LoadWidow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Autolect"
        xmlns:myf="clr-namespace:MyFunctions.ControlsWPF;assembly=MyFunctions"
        mc:Ignorable="d" Height="180" Width="230" Icon="pack://application:,,,/Resources/Images/Logo PNG.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" WindowStyle="None" Background="White">
    <StackPanel Orientation="Vertical" x:Name="grid">
        <Label Name="LB_Participants" Content="0 participants chargés" Background="Gainsboro" HorizontalContentAlignment="Center"/>
        
        <myf:CircleProgress Margin="10 " Mode="Loading" ShowProgress="True"/>
        <Button x:Name="BT_Cancel" Content="Annuler" Margin="0"  Width="209" Height="28" Background="White" BorderBrush="Black" FontSize="16" Click="BT_Cancel_Click"/>
    </StackPanel>
</Window>
